{
  "name": "termnotes",
  "version": "0.0.0",
  "main": "./lib/cli.js",
  "bin": {
    "termnotes": "./lib/cli.js",
    "tn": "./lib/cli.js"
  },
  "files": [
    "lib/**/*",
    "migrations/*"
  ],
  "license": "MIT",
  "scripts": {
    "prepack": "rm -rf lib && run build",
    "types": "run tsc --noEmit",
    "types:watch": "run types --watch",
    "lint": "run eslint src/*.ts ./*.js",
    "build": "run babel --config-file ./babel.config.js ./src/ --out-dir ./lib/ --no-copy-ignored --extensions '.ts'",
    "build:watch": "run build --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@tophat/eslint-config": "^0.8.0",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.36",
    "@types/sqlite3": "^3.1.7",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@github-docs/frontmatter": "^1.3.1",
    "inquirer": "^8.0.0",
    "inquirer-search-list": "^1.2.6",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "resolutions": {
    "typescript": "4.1.5"
  }
}
